<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    @* <title>@ViewData["Title"] - Course_web_project</title> *@
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Course_web_project.styles.css" asp-append-version="true" />




    <link rel="canonical" href="https://getbootstrap.com/docs/5.0/examples/headers/">

    <!-- ВОЗМОЖНО ЭТИ ДВЕ СТРОКИ СТОИТ УДАЛИТЬ, НУЖНО ПРОВЕРИТЬ!  -->
    <!-- Bootstrap CSS  -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>


    <!-- Bootstrap core CSS -->
    <link href="../assets/dist/css/bootstrap.min.css" rel="stylesheet">

    <style>
        .bd-placeholder-img {
            font-size: 1.125rem;
            text-anchor: middle;
            -webkit-user-select: none;
            -moz-user-select: none;
            user-select: none;
        }

        @@media (min-width: 768px) {
            .bd-placeholder-img-lg {
                font-size: 3.5rem;
            }
        }


        .b-example-divider {
            height: 3rem;
            background-color: rgba(0, 0, 0, .1);
            border: solid rgba(0, 0, 0, .15);
            border-width: 1px 0;
            box-shadow: inset 0 .5em 1.5em rgba(0, 0, 0, .1), inset 0 .125em .5em rgba(0, 0, 0, .15);
        }

        .form-control-dark {
            color: #fff;
            background-color: var(--bs-dark);
            border-color: var(--bs-gray);
        }

        .form-control-dark:focus {
            color: #fff;
            background-color: var(--bs-dark);
            border-color: #fff;
            box-shadow: 0 0 0 .25rem rgba(255, 255, 255, .25);
        }

        .bi {
            vertical-align: -.125em;
            fill: currentColor;
        }

        .text-small {
            font-size: 85%;
        }

        .dropdown-toggle {
            outline: 0;
        }



        /* .bg-dark-color {
            background-color: #343a40;
            color: white; /* Если вы хотите также изменить цвет текста, его можно задать здесь */
        } */
    </style>

    <!-- Custom styles for this template -->
    <link href="headers.css" rel="stylesheet">
    <link href="sidebars.css" rel="stylesheet">





</head>
@* <body class="bg-dark-color"> *@
<body>
    <main>
        <header class="p-2 bg-dark text-white">
            <div class="container">
                <div class="d-flex flex-wrap align-items-center justify-content-center justify-content-lg-start">
                    @* <a href="/" class="d-flex align-items-start mb-2 mb-lg-0 text-white text-decoration-none">
                        <svg class="bi me-2" width="40" height="32" role="img" aria-label="Bootstrap"><use xlink:href="#bootstrap" /></svg>
                    </a> *@
                    <a href="/" class="d-flex align-items-center mb-2 mb-lg-0 text-white text-decoration-none">
                        <img src="~/logo_white.png" width="90" height="32" />
                    </a>
                    <h1> </h1>
                    <ul class="nav col-12 col-lg-auto me-lg-auto mb-2 justify-content-center mb-md-0">
                        <li><a href="/" class="link-light px-2 text-white link-underline-opacity-100">Home</a></li>
                        <li><a class="link-light px-2 text-white link-underline-opacity-100" asp-area="" asp-controller="Home" asp-action="AddNewTexture">Добавить текстуру на сайт</a></li>
                        @* <li><a href="#" class="nav-link px-2 text-white">Features</a></li> *@
                        @* <li><a href="#" class="link-light px-2 text-white link-underline-opacity-100">Pricing</a></li> *@
                        <li><a class="link-light px-2 text-white link-underline-opacity-100" asp-area="" asp-controller="Home" asp-action="Pricing">Pricing</a></li>
                        @* <li><a href="#" class="link-light px-2 text-white link-underline-opacity-100">About</a></li> *@
                        <li><a class="link-light px-2 text-white link-underline-opacity-100" asp-area="" asp-controller="Home" asp-action="About">About</a></li>
                    </ul>

                    @* <form class="col-12 col-lg-auto mb-3 mb-lg-0 me-lg-3">
                        <input type="search" class="form-control form-control-dark" placeholder="Search..." aria-label="Search">
                    </form> *@

                    <div class="text-end">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Login">
                                @* <button type="button" class="btn btn-outline-light me-2">Login</button> *@
                            <button type="button" class="btn btn-warning me-2">
                                @(@ViewBag.ActiveUser != null ? "Личный кабинет: " + @ViewBag.ActiveUser : "Авторизация")
                                </button>
                            </a>
                        @* <button type="button" class="btn btn-warning">Sign-up</button> *@
                    </div>
                </div>
            </div>
        </header>


    </main>
    <script src="../assets/dist/js/bootstrap.bundle.min.js"></script>
    <script src="sidebars.js"></script>










